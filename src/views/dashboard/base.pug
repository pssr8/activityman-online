extends ../base.pug

block append styles 
    link(rel="stylesheet", href="/styles/dist/dashboard.css", type="text/css")

block chassis 
    .menu-vertical 
        .links 
            each link in chassis.nav 
                a.link(href=link.pathname) #{link.name} 
    .contenido 
        .cabecera 
            h1.title ActivityMan
        .cuerpo
            h2
                block title
            block cuerpo 