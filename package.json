{
  "name": "activityman-online",
  "version": "1.0.0",
  "description": "The activity manager tool but online (using node js and mongo)",
  "main": "src/index.js",
  "scripts": {
    "dev": "MODE=development PORT=8000 nodemon src/index.js",
    "dev:sass": "sass \"./src/sass/index.scss\":\"./public/styles/dist/index.css\" \"./src/sass/dashboard/_index.scss\":\"./public/styles/dist/dashboard.css\" --watch",
    "show:c": "cat ./terms-and-conditions.txt | less",
    "show:w": "cat ./warranty.txt | less"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simx72/activityman-online.git"
  },
  "keywords": [
    "activities"
  ],
  "author": "Simx72",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Simx72/activityman-online/issues"
  },
  "homepage": "https://github.com/Simx72/activityman-online#readme",
  "dependencies": {
    "console-stamp": "^3.1.1",
    "express": "^4.18.2",
    "express-ip": "^1.0.4",
    "express-session": "^1.17.3",
    "iso-639-1": "^2.1.15",
    "mongoose": "^7.2.2",
    "pug": "^3.0.2",
    "sass": "^1.62.1",
    "vite": "^4.3.9"
  },
  "devDependencies": {
    "nodemon": "^1.14.9"
  },
  "overrides": {
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2",
    "got": "^12.1.0"
  }
